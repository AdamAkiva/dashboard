{
  "name": "dashboard-be",
  "version": "0.0.0",
  "description": "",
  "type": "module",
  "main": "./src/main.ts",
  "scripts": {
    "start:dev": "npx nodemon --config ./nodemon.json ./src/main.ts",
    "test": "NODE_ENV=test node --nolazy --experimental-vm-modules node_modules/jest/bin/jest.js --runInBand --config ./__tests__/jest.config.ts",
    "test:debug": "NODE_ENV=test node --nolazy --inspect-brk=0.0.0.0:$TESTS_DEBUG_PORT --experimental-vm-modules node_modules/jest/bin/jest.js --runInBand --config ./__tests__/jest.config.ts",
    "lint": "npx tsc --project ./tsconfig.json --noEmit && npx eslint \"{src,__tests__}/**/*.ts\"",
    "generate-migrations": "npx drizzle-kit generate:pg --config=./db-migrations/drizzle.config.ts",
    "generate-types": "npx swagger-typescript-api --modular --clean-output --no-client -o ../codegen/ -p src/api-docs/openapi.yaml && mv ../codegen/data-contracts.ts ../codegen/types.ts",
    "build": "npx eslint './src/**/*.ts' && npx tsc --build --verbose ./tsconfig.prod.json",
    "check-global-updates": "npx ncu -g",
    "check-local-updates": "npx ncu",
    "commit-local-updates": "npx ncu -u",
    "check-code-deps": "npx depcheck --ignores=prettier,ts-node",
    "check-cir-deps": "npx dpdm --no-tree --transform --exit-code circular:1 \"{src,__tests__}/**/*.ts\""
  },
  "dependencies": {
    "compression": "~1.7.4",
    "cors": "~2.8.5",
    "drizzle-orm": "~0.29.3",
    "express": "~4.18.2",
    "got": "~14.0.0",
    "helmet": "~7.1.0",
    "postgres": "~3.4.3",
    "zod": "~3.22.4"
  },
  "devDependencies": {
    "@jest/globals": "~29.7.0",
    "@swc/core": "~1.3.102",
    "@swc/jest": "~0.2.29",
    "@types/compression": "~1.7.5",
    "@types/cors": "~2.8.17",
    "@types/express": "~4.17.21",
    "@types/node": "~20.10.6",
    "@types/swagger-ui-express": "~4.1.6",
    "@typescript-eslint/eslint-plugin": "~6.17.0",
    "@typescript-eslint/parser": "~6.17.0",
    "depcheck": "~1.4.7",
    "dpdm": "~3.14.0",
    "drizzle-kit": "~0.20.9",
    "eslint": "~8.56.0",
    "eslint-config-prettier": "~9.1.0",
    "eslint-plugin-drizzle": "~0.2.3",
    "eslint-plugin-security": "~2.1.0",
    "jest": "~29.7.0",
    "jest-junit": "~16.0.0",
    "nodemon": "~3.0.2",
    "npm-check-updates": "~16.14.12",
    "prettier": "~3.1.1",
    "swagger-typescript-api": "~13.0.3",
    "swagger-ui-express": "~5.0.0",
    "ts-node": "~10.9.2",
    "typescript": "~5.3.3",
    "yaml": "~2.3.4"
  },
  "author": "Infinity Labs R&D",
